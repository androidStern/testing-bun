  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend dev:inngest",
    "dev:all": "npm-run-all --parallel dev:frontend dev:backend dev:inngest dev:scrape",
    "dev:frontend": "npx @react-grab/claude-code@latest && bun --bun vite dev",
    "dev:backend": "convex dev",
    "dev:inngest": "inngest dev --no-discovery -u https://efficient-dove-571.convex.site/api/inngest -u http://localhost:3001/api/inngest",
    "dev:scrape": "bun --watch scrape-pipeline/server.ts",
    "predev": "convex dev --until-success && convex dashboard",
    "build": "bun --bun vite build",
    "start": "bun --bun vite start",
    "format": "prettier --write .",
    "lint": "tsc && eslint .  --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "tunnel": "cloudflared tunnel run inngest-dev",
    "infra:up": "docker-compose up -d",
    "infra:down": "docker-compose down"
  },
